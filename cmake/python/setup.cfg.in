[metadata]
name = NekPy
version = 5.5.0
description = Python wrappers for Nektar++
author = David Moxey
author_email = david.moxey@kcl.ac.uk
url = https://www.nektar.info/
license = MIT
classifiers = 
    Programming Language :: Python :: 3
    License :: OSI Approved :: MIT License

[options]
package_dir=
    =${NEKPY_BASE_DIR}
packages=find:

[options.packages.find]
where=${NEKPY_BASE_DIR}

[options.package_data]
* = _*.so
